{
  "extends": "@cmd/tsconfig",
  "compilerOptions": {
    "jsx": "preserve",
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "noEmit": true,
    "plugins": [{ "name": "next" }],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ]
}
